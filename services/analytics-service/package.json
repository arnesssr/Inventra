{
  "name": "@inventra/analytics-service",
  "version": "1.0.0",
  "description": "Analytics and reporting service for Inventra",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t inventra/analytics-service .",
    "docker:run": "docker run -p 8010:8010 inventra/analytics-service"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "zod": "^3.22.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.0",
    "prom-client": "^15.0.0",
    "axios": "^1.6.0",
    "cron": "^3.1.0",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "@types/pg": "^8.10.0",
    "@types/node": "^20.0.0",
    "@types/compression": "^1.7.0",
    "@types/cron": "^2.4.0",
    "typescript": "^5.0.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": ["inventra", "microservice", "analytics", "reporting", "nodejs"],
  "author": "Inventra Team",
  "license": "MIT"
}